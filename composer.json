{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "arcanedev/log-viewer": "~8.0",
        "creativeorange/gravatar": "~1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jorenvanhocht/laravel-share": "^4.0",
        "kalnoy/nestedset": "^5.0",
        "laravel-notification-channels/discord": "^1.3",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.7",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "lukeraymonddowning/honey": "^0.3.2",
        "predis/predis": "^1.1",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/steam": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "syntax/steam-api": "2.3.*",
        "unisharp/laravel-filemanager": "^2.2"
    },
    "require-dev": {
        "arcanedev/route-viewer": "^4.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.15",
        "laravel/homestead": "^12.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "wulfheart/pretty_routes": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
